<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>MVC: @ViewBag.Title</title>
    <!--Skal være uden wwwroot delen hvis du trækker link direkte-->
    <link href="~/css/site.css" rel="stylesheet" />

</head>
<body>
    <header>Welcome to this site</header>
    <section>
        <h2>@ViewBag.Title</h2>
        @RenderBody()
    </section>
    <footer>Dette er footeren</footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js"></script>
</body>
</html>
